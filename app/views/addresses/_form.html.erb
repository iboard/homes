<%=form_for @address do |f| %>
	<%= f.label :address %><br>
	<%= f.text_area :address %><br>
	<%= f.label :zipcode %>
	<%= f.text_field :zipcode %><br>
	<%= f.fields_for :housings do |builder|%>
    <%= render 'housing_fields', f: builder %>
	<%end%>
  <%= link_to_add_fields('Add Person', f, :housings) %>
	<br>
	<%= f.submit "Save" %>
	<% unless @address.valid? %>
		<%= @address.errors.full_messages %>
	<%end%>
<%end%>
